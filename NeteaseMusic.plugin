[Rewrite]
# 私库认证
^https?:\/\/(raw|gist)\.githubusercontent\.com\/{USERNAME} header-del Authorization
^https?:\/\/(raw|gist)\.githubusercontent\.com\/{USERNAME} header-add Authorization "token\x20{TOKEN}"


